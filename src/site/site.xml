<?xml version="1.0" encoding="UTF-8"?>
<project name="Namics AEM commons"
         xmlns="http://maven.apache.org/DECORATION/1.3.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd">

    <publishDate position="bottom" format="dd-MM-yyyy"/>
    <version position="bottom"/>

    <skin>
        <groupId>lt.velykis.maven.skins</groupId>
        <artifactId>reflow-maven-skin</artifactId>
        <version>1.1.1</version>
    </skin>

    <custom>
        <reflowSkin>
            <protocolRelativeURLs>true</protocolRelativeURLs>
            <theme>bootswatch-united</theme>
            <highlightJs>true</highlightJs>
            <smoothScroll>true</smoothScroll>
            <brand>
                <name>Adobe Experience Manager. Opensource. Namics.</name>
                <href>index.html</href>
            </brand>
            <titleTemplate>%2$s | %1$s</titleTemplate>
            <toc>false</toc>
            <topNav>Main|Documentation|Modules</topNav>
            <bottomNav>
                <column>Main|Sub-Modules</column>
                <column>reports</column>
            </bottomNav>

            <bottomDescription>

            </bottomDescription>

            <pages>

            </pages>

        </reflowSkin>
    </custom>

    <body>
        <menu name="Main" inherit="top">
            <item name="Home" href="index.html"/>
        </menu>
        <menu name="Modules" ref="modules" inherit="top"/>
        <menu ref="reports" name="Documentation" inherit="top"/>
    </body>


</project>
